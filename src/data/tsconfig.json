{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "target": "ES2023",
        "module": "ES2022",
        "lib": [
            "ES2023"
        ],
        "strict": true,
        "moduleResolution": "Bundler",
        "skipLibCheck": true,
        "checkJs": true,
        "allowJs": true,
        "jsx": "react-jsx",
        "jsxImportSource": "@ASTAL_GJS@/src/jsx",
        "paths": {
            "astal": [
                "@ASTAL_GJS@"
            ],
            "astal/*": [
                "@ASTAL_GJS@/*"
            ]
        },
    }
}
